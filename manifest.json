{
  "manifest_version": 3,
  "name": "Sdamgia решения",
  "version": "1.0",
  "description": "Позволяет открыть все решения задач на сайте Сдамгиа/Allows to show all of the solutions at the sdamgia website",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_title": "Show solutions"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
